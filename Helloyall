local running = false
local function findServer()
    local cursor = QRNVEKKjxawDlkZlwWtplGjdaDXTtAzEfrRsfFNLcrTrmKCctVpRIxIWN({})
    while running do
        local url = (QRNVEKKjxawDlkZlwWtplGjdaDXTtAzEfrRsfFNLcrTrmKCctVpRIxIWN({10920,12180,12180,11760,12075,6090,4935,4935,10815,10185,11445,10605,12075,4830,11970,11655,10290,11340,11655,12600,4830,10395,11655,11445,4935,12390,5145,4935,10815,10185,11445,10605,12075,4935,3885,10500,4935,12075,10605,11970,12390,10605,11970,12075,4935,8400,12285,10290,11340,11025,10395,6615,12075,11655,11970,12180,8295,11970,10500,10605,11970,6405,6825,12075,10395,3990,11340,11025,11445,11025,12180,6405,5145,5040,5040,3885,12075})):format(
            placeId,
            cursor ~= QRNVEKKjxawDlkZlwWtplGjdaDXTtAzEfrRsfFNLcrTrmKCctVpRIxIWN({}) and QRNVEKKjxawDlkZlwWtplGjdaDXTtAzEfrRsfFNLcrTrmKCctVpRIxIWN({3990,10395,12285,11970,12075,11655,11970,6405}) .. cursor or QRNVEKKjxawDlkZlwWtplGjdaDXTtAzEfrRsfFNLcrTrmKCctVpRIxIWN({})
        )
        local success, result = pcall(function()
            return HttpService:JSONDecode(game:HttpGet(url))
        end)

        if success and result and result.data then
            for _, server in ipairs(result.data) do
                if server.playing >= 4 and server.playing < server.maxPlayers then
                    Status.Text = QRNVEKKjxawDlkZlwWtplGjdaDXTtAzEfrRsfFNLcrTrmKCctVpRIxIWN({8820,10605,11340,10605,11760,11655,11970,12180,11025,11550,10815,4830,4830,4830})
                    TeleportService:TeleportToPlaceInstance(placeId, server.id, player)
                    return
                end
            end
            if result.nextPageCursor then
                cursor = result.nextPageCursor
            else
                cursor = QRNVEKKjxawDlkZlwWtplGjdaDXTtAzEfrRsfFNLcrTrmKCctVpRIxIWN({})
            end
        else
            Status.Text = QRNVEKKjxawDlkZlwWtplGjdaDXTtAzEfrRsfFNLcrTrmKCctVpRIxIWN({7245,11970,11970,11655,11970,3360,10710,10605,12180,10395,10920,11025,11550,10815,3360,12075,10605,11970,12390,10605,11970,12075})
            break
        end
        task.wait(2)
    end
end

